{{#each bookmark}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <p> Description {{description}} </p>
    {{#if websites}}
    <a href="/bookmark/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    {{/if}}
    

    <a href="/removeBookmark/{{id}}" class="removeBookmark ui icon button">
      <i class="icon trash"></i>
    </a>
    <div class="imgholder clearfix">
      <img src = "{{img}}">
    </div>
  </section>
{{/each}}