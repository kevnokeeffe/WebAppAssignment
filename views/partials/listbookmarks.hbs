{{#each bookmark}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    <p> Total Duration: {{duration}} </p>
    <a href="/bookmark/{{id}}" class="ui icon button">
      <i class="icon folder open"></i>
    </a>
    <a href="/dashboard/removeBookmark/{{id}}" class="ui icon button">
      <i class="icon trash"></i>
    </a>
    <div class="imgholder clearfix">
      <img src = "{{img}}">
    </div>
  </section>
{{/each}}